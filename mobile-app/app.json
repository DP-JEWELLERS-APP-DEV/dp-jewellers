{
  "expo": {
    "name": "DP Jewellers",
    "slug": "pn-rn-JewelleryEmpire",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/apk-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.dpjewellers.app",
      "buildNumber": "1.0.1",
      "supportsTablet": false
    },
    "android": {
      "package": "com.dpjewellers.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/apk-logo-android.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "93795a8f-8cab-4d4a-9f83-713974d1b672"
      }
    },
    "owner": "officialaakash"
  }
}
